{
  "name": "citool",
  "version": "0.0.0",
  "description": "Workrave CI/CD tools",
  "bin": {
    "citool": "./bin/citool.ts"
  },
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rimraf ./dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "keywords": [],
  "author": "Rob Caelers",
  "license": "MIT",
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "@oclif/plugin-commands": "^2.2.2",
    "async": "^3.2.4",
    "aws-sdk": "^2.1356.0",
    "isomorphic-git": "^1.21.0",
    "js-yaml": "^4.1.0",
    "mergician": "^1.0.3",
    "moment": "^2.29.4",
    "node-fetch": "^3.3.0",
    "nunjucks": "^3.2.4",
    "remark-parse": "^10.0.1",
    "semver": "^7.3.8",
    "source-map-support": "^0.5.21",
    "unherit": "^3.0.1",
    "unified": "^10.1.2",
    "word-wrap": "^1.2.4",
    "xtend": "^4.0.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.25",
    "@types/async": "^3.2.16",
    "@types/chai": "^4",
    "@types/eslint": "^8.4.10",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/nunjucks": "^3.2.1",
    "@types/prettier": "^2.7.2",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.13",
    "@types/source-map-support": "^0.5.6",
    "@types/tar-fs": "^2.0.1",
    "@types/unzipper": "^0.10.5",
    "@types/xtend": "^4.0.4",
    "chai": "^4",
    "eslint": "^8.31.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10",
    "oclif": "^3",
    "prettier": "^2.8.2",
    "rimraf": "^4.0.4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "citool",
    "dirname": "citool",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-commands"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "engines": {
    "node": ">=16.6.0"
  },
  "files": [
    "/bin",
    "/dist"
  ],
  "types": "dist/index.d.ts"
