#: ../backend/src/DistributionSocketLink.cc:712
#, c-format
msgid "Removing client %s:%d."
msgstr "%s:%d kliens eltávolítása."

#. Closing direct connection.
#: ../backend/src/DistributionSocketLink.cc:752
#, c-format
msgid "Disconnecting %s:%d."
msgstr "Kapcsolat bontása a következővel: %s:%d."

#. It's a remote client. mark it master.
#: ../backend/src/DistributionSocketLink.cc:922
#, c-format
msgid "Client %s:%d is now master."
msgstr "A következő kliens az elsődleges: %s:%d."

#. Its ME!
#: ../backend/src/DistributionSocketLink.cc:929
msgid "I'm now master."
msgstr "Én vagyok az elsődleges."

#: ../backend/src/DistributionSocketLink.cc:1260
#, c-format
msgid "Client %s:%d saying hello."
msgstr "%s:%d kliens „hello”-t küldött."

#. Duplicate client. inform client that it's bogus and close.
#: ../backend/src/DistributionSocketLink.cc:1276
#: ../backend/src/DistributionSocketLink.cc:1406
#: ../backend/src/DistributionSocketLink.cc:1650
#, c-format
msgid "Client %s:%d is duplicate."
msgstr "A következő kliens többször szerepel: %s:%d."

#. Incorrect password.
#: ../backend/src/DistributionSocketLink.cc:1286
#, c-format
msgid "Client %s:%d access denied."
msgstr "Hozzáférés megtagadva: %s:%d"

#: ../backend/src/DistributionSocketLink.cc:1356
#, c-format
msgid "Client %s:%d signed off."
msgstr "%s:%d kliens kijelentkezett."

#: ../backend/src/DistributionSocketLink.cc:1445
#, c-format
msgid "Client %s:%d is welcoming us."
msgstr "%s:%d kliens üdvözöl minket."

#: ../backend/src/DistributionSocketLink.cc:1687
#, c-format
msgid "Requesting master status from %s:%d."
msgstr "Elsődleges állapot igénylése a következőtől: %s:%d."

#: ../backend/src/DistributionSocketLink.cc:1701
#, c-format
msgid "Client timeout from %s:%d."
msgstr "Kliens időtúllépés: %s:%d."

#: ../backend/src/DistributionSocketLink.cc:1723
#, c-format
msgid "Rejecting master request from client %s:%d."
msgstr "%s:%d kliens elsődlegességi kérelme elutasítva."

#: ../backend/src/DistributionSocketLink.cc:1729
#, c-format
msgid "Acknowledging master request from client %s:%d."
msgstr "%s:%d kliens elsődlegességi kérelme elfogadva."

#: ../backend/src/DistributionSocketLink.cc:1780
#, c-format
msgid "Non-master client %s:%d rejected master request."
msgstr "%s:%d nem elsődleges kliens elutasította elsődlegességi kérelmét."

#: ../backend/src/DistributionSocketLink.cc:1785
#, c-format
msgid "Client %s:%d rejected master request, delaying."
msgstr "%s:%d kliens elutasította elsődlegességi kérelmét, késleltetés…"

#. gint count =
#: ../backend/src/DistributionSocketLink.cc:1856
#, c-format
msgid "Client %s is now the new master."
msgstr "%s kliens az elsődleges."

#: ../backend/src/DistributionSocketLink.cc:1978
msgid "Network operation started."
msgstr "Hálózati műveletek elkezdve."

#: ../backend/src/DistributionSocketLink.cc:1995
msgid "Accepted new client."
msgstr "Új kliens elfogadva."

#: ../backend/src/DistributionSocketLink.cc:2061
#, c-format
msgid "Client %s:%d read error, closing."
msgstr "Olvasási hiba: %s:%d. Befejezés."

#: ../backend/src/DistributionSocketLink.cc:2067
#: ../backend/src/DistributionSocketLink.cc:2141
#, c-format
msgid "Client %s:%d closed connection."
msgstr "%s:%d kliens befejezte a kapcsolatot."

#: ../backend/src/DistributionSocketLink.cc:2108
#, c-format
msgid "Client %s:%d connected."
msgstr "%s:%d kliens kapcsolódott."

#: ../backend/src/DistributionSocketLink.cc:2148
#, c-format
msgid "Could not connect to client %s:%d."
msgstr "Sikertelen kapcsolódás %s:%d klienshez."

