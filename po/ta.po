#, c-format
msgid "Client %s signed off."
msgstr "கிளையண்ட் %s கையொப்பமிட்டுள்ளார்."

#. gint port =
#: libs/core/src/DistributionSocketLink.cc:1525
#, c-format
msgid "Client %s is welcoming us."
msgstr "கிளையண்ட் %s எங்களை வரவேற்கிறார்."

#: libs/core/src/DistributionSocketLink.cc:1767
#, c-format
msgid "Requesting master status from %s."
msgstr "%s இலிருந்து முதன்மை நிலையைக் கோருகிறது."

#: libs/core/src/DistributionSocketLink.cc:1780
#, c-format
msgid "Client timeout from %s."
msgstr "%s இலிருந்து கிளையன்ட் நேரம் முடிந்தது."

#: libs/core/src/DistributionSocketLink.cc:1806
#, c-format
msgid "Rejecting master request from client %s."
msgstr "கிளையன்ட் %s இலிருந்து முதன்மை கோரிக்கையை நிராகரிக்கிறது."

#: libs/core/src/DistributionSocketLink.cc:1811
#, c-format
msgid "Acknowledging master request from client %s."
msgstr "கிளையன்ட் %s இலிருந்து முதன்மை கோரிக்கையை ஒப்புக்கொள்கிறது."

#: libs/core/src/DistributionSocketLink.cc:1864
#, c-format
msgid "Non-master client %s rejected master request."
msgstr "முதன்மை அல்லாத கிளையன்ட் %s நிராகரிக்கப்பட்ட முதன்மை கோரிக்கை."

#: libs/core/src/DistributionSocketLink.cc:1868
#, c-format
msgid "Client %s rejected master request, delaying."
msgstr "கிளையண்ட் %s முதன்மை கோரிக்கையை நிராகரித்தார், தாமதப்படுத்துகிறார்."

#. gint count =
#: libs/core/src/DistributionSocketLink.cc:1942
#, c-format
msgid "Client %s is now the new master."
msgstr "கிளையன்ட் %s இப்போது புதிய மாஸ்டர்."

#: libs/core/src/DistributionSocketLink.cc:2070
msgid "Network operation started."
msgstr "நெட்வொர்க் செயல்பாடு தொடங்கியது."

